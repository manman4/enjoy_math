   "source": [
    "import is_prime as ispr\n",
    "import math\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x1, y1 = [], []\n",
    "pi = 0\n",
    "\n",
    "for n in range(2, 1000001):\n",
    "    if ispr.is_prime(n):\n",
    "        pi += 1\n",
    "\n",
    "    if n % 100 == 0:\n",
    "        x1.append(n)\n",
    "        y1.append(n / (math.log(n) * pi))\n",
    "\n",
    "\n",
    "plt.plot(x1, y1)\n",
    "plt.show()"
   ]
  }
 ],
